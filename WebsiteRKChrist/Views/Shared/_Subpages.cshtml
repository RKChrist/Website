@{
    string text = ViewBag.Product;
    if (text == null && text == default)
    {
        ViewBag.Product = "hidden";
    }
    string terms = ViewBag.Terms;
    if (terms == null && terms == default)
    {
        ViewBag.Terms = "hidden";
    }
    string objectives = ViewBag.Objectives;
    if (objectives == null && objectives == default)
    {
        ViewBag.Objectives = "hidden";
    }
    string material = ViewBag.Material;
    if (material == null && material == default)
    {
        ViewBag.Material = "hidden";
    }

}


